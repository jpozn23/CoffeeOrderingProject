<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CoffeeOrderingApp.MenuPage"
             BackgroundColor="Ivory">
    <ContentPage.Content >
        <StackLayout>
            <Image Source="coffee_menu.png" HeightRequest="600" Margin="3,-36,3,0"/>
            <Button x:Name="BackToHomeButton" Clicked="BackToHomeButton_Clicked"
                    Text="Back To Home" Margin="5,-35,5,0" TextColor="White"
                    HeightRequest="60" BackgroundColor="#4681f4" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>